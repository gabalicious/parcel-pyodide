{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "versionString",

  "default_locale": "en",
  "description": "A plain text description",

  "background": {
    "persistent": true,
    "service_worker": ""
  },
  "chrome_ui_overrides": {
    "bookmarks_ui": {
      "remove_bookmark_shortcut": true,
      "remove_button": true
    }
  },
  "content_scripts": [
    {
      "js": ["index.js", "my-script.js"]
    }
  ],

  "web_accessible_resources": ["index.js", "my-script.js"]
}
